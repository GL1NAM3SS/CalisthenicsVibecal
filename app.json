{
  "expo": {
    "name": "Calisthenics Tracker",
    "slug": "calisthenics-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calisthenics.tracker",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "android": {
      "package": "com.calisthenics.tracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": ["VIBRATE", "WAKE_LOCK"]
    },
    "plugins": [
      "expo-sqlite",
      "expo-haptics",
      "expo-notifications",
      "expo-sharing",
      "expo-file-system",
      "expo-keep-awake"
    ]
  }
}
